{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ESNext", "DOM"],
    "types": ["vite/client", "@cloudflare/workers-types", "hono"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx/dom",
    "paths": {
      "react": ["hono/jsx/dom"],
      "react-dom": ["hono/jsx/dom"],
      "@/*": ["./*"],
    },
    "baseUrl": "."
  }
}
